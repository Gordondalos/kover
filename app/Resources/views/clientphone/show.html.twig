{% extends 'base.html.twig' %}

{% block body %}
    <h1>ClientPhone</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ clientPhone.id }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ clientPhone.phone }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
